import UIKit

extension UIColor{
    
    class func getBlueInspireColor() -> UIColor{
        return UIColor.init(red: 2/255.0, green: 49/255.0, blue: 95/255.0, alpha: 1.0)
    }
    
}

/*
How to use ?
Use this like other predefined UIColor of Xcode

*/
